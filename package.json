{
  "name": "fish-icons",
  "version": "1.0.2",
  "description": "React components of fish-icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "create": "tsx build/create.ts",
    "optimize": "svgo -f ./src/svg -o ./src/svg",
    "build": "npm run clean && npm run create && tsx ./build/index.ts && tsc"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fish-popo/fish-icons.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-plugin-jsonc": "^2.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/prettier": "^3.0.0",
    "camelcase": "^8.0.0",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "esbuild": "^0.19.8",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "prettier": "^3.2.1",
    "tsx": "^4.5.0",
    "typescript": "^5.3.2",
    "react-dom": "18.3.1",
    "react": "18.3.1",
    "classnames": "^2.5.1",
    "rimraf": "^3.0.2",
    "svgo": "^3.0.4",
    "jsdom": "^23.2.0"
  }
}
